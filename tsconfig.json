{
  "compileOnSave": false,
  "compilerOptions": {
    "jsx": "preserve",
    "baseUrl": "./",
    "outDir": "./dist/",
    "sourceMap": true,
    "allowJs": true,
    "declaration": false,
    "module": "commonjs",
    "alwaysStrict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "downlevelIteration": true,
    "importHelpers": true,
    "target": "es2019",
    "typeRoots": [
      "./node_modules/@types"
    ],
    "lib": [
      "es2019",
      "dom"
    ],
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "isolatedModules": true
  },
  "include": [
    "/src/pages/index.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
